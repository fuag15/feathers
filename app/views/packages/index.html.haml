- content_for :title do
  #{@owner.name} Package List
%table.table.table-striped
  %thead
    %tr
      %th
        file
      %th
        version
  %tbody
    - @packages.each do |package|
      %tr
        %td= link_to package.file.url, [package.owner, package]
        %td= package.version
.btn-group.pull-left
  = link_to 'create package', owner_aware_new_package_path(@owner), class: 'btn btn-primary'
  = link_to 'back', @owner, class: 'btn'